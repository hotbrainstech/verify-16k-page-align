{
  "name": "verify-16k-page-align",
  "version": "0.0.1",
  "description": "Shell script and npm package to verify Android APK/AAB/APEX native libraries are 16KB or 64KB page aligned for Android 15+ compatibility",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "author": "hotbrainstech",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "lint": "shellcheck src/verify-16k-page-align.sh",
    "test": "./src/verify-16k-page-align.sh --help",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "verify-16k-page-align": "./src/verify-16k-page-align.sh"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "src/**/*"
  ],
  "os": [
    "linux",
    "!darwin",
    "!win32"
  ],
  "bundleDependencies": [
    "@types/node"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hotbrainstech/verify-16k-page-align.git"
  },
  "keywords": [
    "android",
    "android-15",
    "16kb-pages",
    "page-alignment",
    "apk",
    "aab",
    "apex",
    "native-libraries",
    "elf",
    "objdump",
    "react-native",
    "npm",
    "shell-script",
    "ci-cd"
  ],
  "bugs": {
    "url": "https://github.com/hotbrainstech/verify-16k-page-align/issues"
  },
  "homepage": "https://github.com/hotbrainstech/verify-16k-page-align#readme",
  "devDependencies": {
    "@types/node": "^24.5.2",
    "concurrently": "^9.2.1"
  }
}